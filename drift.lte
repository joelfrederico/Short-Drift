! Charge of one electron
! Will be multiplied by the number
! of particles via alterelements in
! the elegant deck file.
C: CHARGE,PER_PARTICLE=1.6e-19

Q: EMATRIX,L=0.01

Q2X:   LINE =(Q   , Q)
Q4X:   LINE =(Q2X , Q2X)
Q8X:   LINE =(Q4X , Q4X)
Q16X:  LINE =(Q8X , Q8X)
Q32X:  LINE =(Q16X, Q16X)
Q64X:  LINE =(Q32X, Q32X)
Q128X: LINE =(Q64X, Q64X)

MAINBEAMLINE: LINE=(C,Q128X)
