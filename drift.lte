! Charge of one electron
! Will be multiplied by the number
! of particles via alterelements in
! the elegant deck file.
C: CHARGE,PER_PARTICLE=1.6e-19

XC1: HKIC,L=1e-1
DRIFT: DRIF,L=1,ORDER=2
QF: QUAD, L=3E-01,K1=5E-01
QD: QUAD, L=3E-01,K1=-5E-01
BPM: MONI
mainbeamline: LINE=(C,QF,XC,DRIFT,QD,XC,DRIFT,QF,XC,DRIFT,QD,XC,DRIFT,QF,XC)
